<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="APIWoood.Logic.Models"
                   assembly="APIWoood.Logic">
  <class name="Dashboard" table="EEK_WEB_DASHBOARD" lazy="false">
    <id name="Id">
      <generator class="identity"/>
    </id>
    <property name="DebCode"/>
    <property name="ItemCode"/>
    <property name="EAN"/>
    <property name="Quantity"/>
    <property name="Amount"/>
    <property name="Type"/>
    <property name="AmountSortOrder"/>
    <property name="QuantitySortOrder"/>
    <property name="SysModified"/>
  </class>

</hibernate-mapping>